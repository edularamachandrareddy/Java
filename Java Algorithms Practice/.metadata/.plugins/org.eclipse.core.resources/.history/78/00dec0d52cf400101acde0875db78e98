
public class CompareY implements Comparable<Point>{
	private double x = 0.0;
	private double y = 0.0;

	public double getX(){
		return this.x;
	}
	public void setX(double newX){
		this.x = newX;
	}
	public double getY(){
		return this.y;
	}
	public void setY(double newY){
		this.y = newY;
	}

	public String toString(){
		return "x" + this.x + " y" + this.y;
	}
	
	//constructor
	public CompareY(double newX, double newY){
		this.x = newX;
		this.y = newY;
	}
	
	@Override
	public int compareTo(Point otherPoint) {
		double differenceX = this.x - otherPoint.getX();
		double differenceY = this.y - otherPoint.getY();
		if(differenceY != 0.0)
			return (int)differenceY;
		else
			return (int)differenceX;
	}

}
